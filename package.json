{
  "name": "moodle-scrape",
  "version": "3.1.4",
  "description": "Scrape data from Moodle sites",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "module": "dist/index.mjs",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "rimraf dist && tsc && npm run build:esm && npm run build:docs",
    "build:esm": "gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
    "build:docs": "rimraf docs && typedoc src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nizefoo/moodle-scrape.git"
  },
  "author": "nizefoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nizefoo/moodle-scrape/issues"
  },
  "homepage": "https://moodle-scrape.nize.foo",
  "devDependencies": {
    "gen-esm-wrapper": "^1.1.3",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  }
}
