{
  "name": "moodle-scrape",
  "version": "3.0.0",
  "description": "Scrape data from Moodle sites",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "module": "dist/index.mjs",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "rimraf dist && tsc && npm run build:esm && npm run build:docs",
    "build:esm": "gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
    "build:docs": "rimraf docs && typedoc src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nizewn/moodle-scrape.git"
  },
  "author": "nizewn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nizewn/moodle-scrape/issues"
  },
  "homepage": "https://nizewn.github.io/moodle-scrape/",
  "devDependencies": {
    "gen-esm-wrapper": "^1.1.3",
    "rimraf": "^4.1.2",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  }
}
